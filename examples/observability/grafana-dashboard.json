{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "uid": "pgcdc-overview",
  "title": "pgcdc Overview",
  "description": "PostgreSQL CDC pipeline monitoring",
  "tags": ["pgcdc", "cdc", "postgresql"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "10s",
  "schemaVersion": 39,
  "version": 1,
  "templating": {
    "list": [
      {
        "name": "adapter",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "query": "label_values(pgcdc_events_delivered_total, adapter)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Events Received Rate",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(pgcdc_events_received_total[5m])",
          "legendFormat": "received",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Events Delivered Rate by Adapter",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(pgcdc_events_delivered_total{adapter=~\"$adapter\"}[5m])",
          "legendFormat": "{{ adapter }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Events Dropped Rate",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "rate(pgcdc_events_dropped_total{adapter=~\"$adapter\"}[5m])",
          "legendFormat": "{{ adapter }}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "WAL & Replication",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Slot Lag Bytes",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 524288000 },
              { "color": "red", "value": 2147483648 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "pgcdc_slot_lag_bytes",
          "legendFormat": "slot lag",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Checkpoint LSN",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
