        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "pgcdc_checkpoint_lsn",
          "legendFormat": "checkpoint LSN",
          "refId": "A"
        },
        {
          "expr": "pgcdc_cooperative_checkpoint_lsn",
          "legendFormat": "cooperative LSN",
          "refId": "B"
        }
      ]
    },
    {
      "type": "state-timeline",
      "title": "Backpressure State",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 2 }
            ]
          },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Green", "color": "green" } } },
            { "type": "value", "options": { "1": { "text": "Yellow", "color": "yellow" } } },
            { "type": "value", "options": { "2": { "text": "Red", "color": "red" } } }
          ]
        },
        "overrides": []
      },
      "options": {
        "showValue": "auto",
        "alignValue": "left",
        "mergeValues": true
      },
      "targets": [
        {
          "expr": "pgcdc_backpressure_state",
          "legendFormat": "backpressure",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Backpressure Throttle Duration",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(pgcdc_backpressure_throttle_duration_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(pgcdc_backpressure_throttle_duration_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "B"
        }
      ]
    },
    {
